{
  "name": "paris-mechanical-capacity-manager",
  "version": "1.0.0",
  "description": "Capacity management and cash flow forecasting system for Paris Plumbing and Heating",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "playwright test",
    "test:all": "npm run test:ci && npm run test:e2e",
    "test:smoke": "npm run test:e2e -- --grep @smoke",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio",
    "analyze": "ANALYZE=true next build",
    "env:check": "tsx scripts/check-env.ts",
    "monday:sync": "tsx scripts/monday-sync.ts",
    "deploy:staging": "vercel --env=preview",
    "deploy:production": "vercel --prod",
    "monitor:memory": "node --inspect scripts/memory-monitor.js"
  },
